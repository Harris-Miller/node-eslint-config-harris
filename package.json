{
  "name": "eslint-config-harris",
  "version": "2.1.0",
  "description": "Harris' ESLint config",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Harris-Miller/node-eslint-config-harris.git"
  },
  "scripts": {
    "bump": "bump --prompt --commit --tag",
    "bump:rc": "yarn bump --preid=rc'",
    "bump:beta": "yarn bump --preid=beta'",
    "lint": "eslint .",
    "test": "eslint tests/**/*.js tests/**/*.jsx"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "typescript",
    "tslint",
    "harris"
  ],
  "author": "Harris Miller <harrismillerconsulting@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.14.0",
    "typescript": "^4.1.2",
    "version-bump-prompt": "^6.1.0"
  },
  "peerDependencies": {
    "eslint": "^7.14.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.0"
  }
}
